{
  "name": "stinky-socks",
  "version": "1.0.0",
  "description": "App to import DevEx metrics of different kind.",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node src/app.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint ./src --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update-snapshots": "jest --updateSnapshot",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/metrics-db/MetricsDB.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/metrics-db/MetricsDB.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/metrics-db/MetricsDB.ts"
  },
  "keywords": [
    "devex",
    "jenkins",
    "bitbucket",
    "github"
  ],
  "author": "Fedor Shchudlo",
  "license": "Apache 2.0",
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.4.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.0",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.8.1"
  }
}
